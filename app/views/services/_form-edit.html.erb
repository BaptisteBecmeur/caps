<div class="row">
  <div class= "col-md-8 col-md-offset-2 form-signup">
    <h2 class="text-center">Editer votre service</h2>
    <%= simple_form_for(@service) do |f| %>
       <%= f.input :tag, label: "Sélectionnez un type de prestation", collection: Service::TAGS, selected: Service::TAGS.first %>
        <%= f.input :pseudo, placeholder: 'Votre nom de scéne'  %>
        <%= f.input :introduction %>
        <%= f.input :content, label: 'Bio', placeholder: "Parlez de votre vie artistique" %>
        <%= f.input :address, label: 'Adresse', placeholder: "Ex : 31 quai de la Seine " %>
        <%= f.input :zip_code, label: 'Code Postal' %>
        <%= f.input :city, label: 'Ville' %>
        <%= f.input :country, priority: [ 'FR' ], label: 'Pays' %>
        <%= f.input :cover, label: 'Ajouter une photo de profil' %>
        <%= f.input :cover_cache, as: :hidden %>
      <%= f.submit "Publier", class: "btn btn-default text-center" %>
    <% end %>
    <h4>
      <%= link_to '< Retour', :back %>
    </h4>
  </div>
</div>

<%= content_for(:after_js) do %>
  <script src="https://maps.googleapis.com/maps/api/js?libraries=places&amp;key=AIzaSyCvegYCjKjhUpT8Op8O8BQMNJVYKsmqPSg"></script>
<% end %>
